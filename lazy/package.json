{
  "name": "react-app",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "concurrently 'yarn:lint:*'",
    "lint:js": "eslint '**/*.{ts,js,tsx,jsx}'",
    "lint:format": "prettier --check '**/*.{ts,js,tsx,jsx,json,md,yml}'",
    "lint:styles": "stylelint --ignore-path .eslintignore '**/*.scss'",
    "fix": "concurrently 'yarn:fix:*'",
    "fix:js": "eslint --fix '**/*.{ts,js}'",
    "fix:format": "prettier --write '**/*.{ts,js,tsx,jsx,json,md,yml}'",
    "fix:styles": "stylelint --fix --ignore-path .eslintignore '**/*.scss'"
  },
  "dependencies": {
    "modern-normalize": "^1.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.3",
    "@types/node": "^18.0.0",
    "@types/prettier": "^2.6.3",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-react": "^1.3.0",
    "concurrently": "^7.2.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "postcss": "^8.4.14",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "sass": "^1.52.3",
    "stylelint": "^14.9.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}
