{
  "name": "animate-scroll",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "concurrently \"yarn:lint:*\"",
    "lint:eslint": "eslint --ignore-path ../.eslintignore \"**/*.{ts,js}\"",
    "lint:prettier": "prettier --check --ignore-path ../.eslintignore \"**/*.{ts,js,json,md,yml}\"",
    "lint:styles": "stylelint --ignore-path ../.eslintignore \"**/*scss\"",
    "fix": "concurrently \"yarn:fix:*\"",
    "fix:eslint": "eslint --fix --ignore-path ../.eslintignore \"**/*.{ts,js}\"",
    "fix:prettier": "prettier --ignore-path ../.eslintignore --write \"**/*.{ts,js,json,md,yml}\"",
    "fix:styles": "stylelint --ignore-path ../.eslintignore \"**/*scss\" --fix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "concurrently": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.3.2",
    "sass": "^1.37.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "typescript": "^4.3.2",
    "vite": "^2.4.4"
  },
  "dependencies": {
    "gsap": "^3.7.1"
  }
}
