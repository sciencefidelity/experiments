{
  "name": "syntax-highlighter",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "concurrently \"yarn:lint:*\"",
    "lint:eslint": "eslint --ignore-path ../.eslintignore \"**/*.{ts,js}\"",
    "lint:prettier": "prettier --check --ignore-path ../.prettierignore \"**/*.{ts,js,json,md,yml}\"",
    "fix": "concurrently \"yarn:fix:*\"",
    "fix:eslint": "eslint --fix --ignore-path ../.eslintignore \"**/*.{ts,js}\"",
    "fix:prettier": "prettier --ignore-path ../.prettierignore --write \"**/*.{ts,js,json,md,yml}\""
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "concurrently": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.3.2",
    "sass": "^1.37.0",
    "typescript": "^4.3.5",
    "vite": "^2.4.4"
  }
}
